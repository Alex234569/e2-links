{
  "manifest_version": 2,
  "name": "E2 links",
  "version": "1.0",
  "description": "Useful links for e2 developers",
  "icons": {
    "48": "images/icon.png"
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';"
}
